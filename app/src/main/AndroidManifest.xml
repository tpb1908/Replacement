<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.anapp.tpb.replacement"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity
            android:name=".Home.Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Setup.DataPresentation.ClassTimeCollector"
            android:taskAffinity=""
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Setup.DataPresentation.SubjectCollector"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Setup.DataCollection.SubjectInput"
            android:theme="@style/Theme.Sliding.Light">
        </activity>
        <activity
            android:name=".Home.Input.HomeworkInput"
            android:taskAffinity=""
            android:theme="@style/Theme.Sliding.Light">
        </activity>
        <activity
            android:name=".Setup2.Collectors.ClassCollector"
            android:taskAffinity=""
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Setup2.Input.ClassInput"
            android:taskAffinity=""
            android:theme="@style/Theme.Sliding.Light">
        </activity>
        <activity
            android:name=".Setup.IntroActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".Setup2.TestActivity"
            android:taskAffinity=""
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Home.Splash"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>